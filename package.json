{
  "name": "testx-http-keywords",
  "version": "0.12.0",
  "description": "Keywords to send simple http requests and check the responses using the testx library.",
  "main": "index.coffee",
  "author": "Greyarch BV",
  "license": "ISC",
  "dependencies": {
    "coffee-errors": "^0.8.6",
    "jsonpath-plus": "^0.15.0",
    "lodash": "^4.17.4",
    "parse-key-value": "^1.0.0",
    "request": "^2.73.0"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "protractor": "^5.1.1",
    "testx": "^2.4.2"
  },
  "scripts": {
    "pretest": "webdriver-manager update",
